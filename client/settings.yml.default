defaults: &defaults
  consumer:
    - key: *consumer_key*
      secret: *consumer_secret*
  secret_key: *secret_key*
development:
  <<: *defaults
  worker_number: 0
  db_proxy_host: localhost
  db_proxy_port: 9001
production:
  <<: *defaults
  worker_number: <%= ENV["ACLOG_WORKER_NUMBER"] %>
  db_proxy_host: localhost
  db_proxy_port: 9001

